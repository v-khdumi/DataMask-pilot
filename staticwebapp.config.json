{
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/api/*"]
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-inline' 'unsafe-eval'"
    }
  }
  